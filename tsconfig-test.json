{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "rootDir": "./src",
    "resolveJsonModule": true,
    "strict": false,
    "declaration": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": false,
    "removeComments": false,
    "baseUrl": ".",
    "paths": {
      "*": [
        "*"
      ]
    },
    "noUnusedLocals": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "compileOnSave": false,
  "buildOnSave": false
}
